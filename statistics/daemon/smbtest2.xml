<service>
	<id>smbtest2.exe</id>
	<name>SMB-test-2</name>
	<description>Writes SMB-test results to mongo ang get statistics</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>D:\smb-test\statistics\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\smb-test\statistics\app.js</argument>
	<argument>--log</argument>
	<argument>SMB-test-2 wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>D:\smb-test\statistics</workingdirectory>
</service>